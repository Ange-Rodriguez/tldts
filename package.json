{
  "name": "tldts",
  "description": "Library to work against complex domain names, subdomains and URIs.",
  "version": "5.2.1",
  "homepage": "https://github.com/remusao/tldts",
  "author": "RÃ©mi Berson",
  "contributors": [
    "Thomas Parisot (https://oncletom.io)"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/remusao/tldts.git"
  },
  "bugs": {
    "url": "https://github.com/remusao/tldts/issues"
  },
  "engines": {
    "node": ">= 6"
  },
  "browser": "dist/tldts.umd.min.js",
  "main": "build/cjs/tldts.js",
  "module": "build/es6/tldts.js",
  "types": "dist/tldts.d.ts",
  "files": [
    "build",
    "dist"
  ],
  "license": "MIT",
  "bin": {
    "tldts": "bin/cli.js"
  },
  "scripts": {
    "clean": "rm -rfv dist build",
    "lint": "tslint -c tslint.json 'lib/**/*.ts'",
    "update": "git submodule foreach git pull origin master && ts-node -O '{\"module\": \"commonjs\"}' ./bin/update.ts",
    "build-cjs": "tsc -p . --outDir build/cjs --module commonjs",
    "build-es6": "tsc -p . --outDir build/es6 --module esnext",
    "build": "concurrently 'npm run build-cjs' 'npm run build-es6'",
    "prebuild": "npm run clean",
    "bundle": "rollup -c rollup.config.ts",
    "prebundle": "npm run build",
    "prepack": "npm run bundle",
    "test": "jest --coverage --no-cache ./test/",
    "pretest": "npm pack",
    "benchmark": "make -C bench",
    "prebenchmark": "npm pack"
  },
  "dependencies": {},
  "devDependencies": {
    "@ampproject/rollup-plugin-closure-compiler": "^0.9.0",
    "@types/jest": "^24.0.12",
    "@types/node": "^12.0.2",
    "benchmark": "^2.1.4",
    "chalk": "^2.4.2",
    "concurrently": "^4.1.1",
    "coveralls": "^3.0.3",
    "jest": "^24.8.0",
    "rollup": "^1.11.3",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5"
  },
  "keywords": [
    "tld",
    "sld",
    "domain",
    "subdomain",
    "subdomain",
    "hostname",
    "browser",
    "uri",
    "url",
    "domain name",
    "public suffix",
    "url parsing",
    "typescript"
  ]
}
